# Vedanta BYOK AI Platform - Environment Variables
# Copy this file to .env and fill in your Firebase configuration

# Firebase Configuration (Required for BYOK authentication)
# Get these from https://console.firebase.google.com/
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=your_app_id_here

# No AI API Keys needed in environment - users provide their own!
# This is a pure BYOK (Bring Your Own Key) system

# Optional: Analytics and monitoring
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# Development settings
VITE_DEV_MODE=true
